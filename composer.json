{
    "name": "shamim/dewan-multilang-slug",
    "description": "It takes a string, a model,  a filed, and a divider, and returns a unique string with a number or random string appended to it if the slug already exists in the database.",
    "type": "composer-plugin",
    "license": "MIT",
    "autoload": {
        "psr-4": {
            "Shamim\\DewanMultilangSlug\\": "src/"
        }
    },
    "authors": [
        {
            "name": "shamim",
            "email": "shamimdewan343@gmail.com"
        }
    ],
    "minimum-stability": "dev",
    "require": {
        "php": ">=7.4"
    },
    "extra": {
        "laravel": {
            "providers": [
                "Shamim\\DewanMultilangSlug\\MultilangSlugServiceProvider"
            ],
            "aliases": {
                "MultilangSlug": "Shamim\\DewanMultilangSlug\\Facade\\MultilangSlug"
            }
        }
    }
}